    "@fullcalendar/interaction": "6.1.14",
    "@fullcalendar/timegrid": "6.1.14",
    "@fullcalendar/vue3": "6.1.14",
    "@mdi/font": "7.4.47",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "bcryptjs": "2.4.3",
    "cookiejs": "2.1.3",
    "mysql2": "3.10.1",
    "nodemailer": "6.9.13",
    "ofetch": "1.3.4",
    "vue": "3.4.29",
    "vue-router": "4.3.3",
    "vuetify": "3.6.9"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.3.3",
    "@nuxt/eslint": "0.3.13",
    "@nuxtjs/google-fonts": "3.2.0",
    "@nuxtjs/i18n": "8.3.1",
    "@pinia/nuxt": "0.5.1",
    "@stylistic/eslint-plugin": "2.2.0",
    "@vite-pwa/nuxt": "0.8.0",
    "cross-env": "7.0.3",
    "eslint": "9.5.0",
    "eslint-plugin-vue": "9.26.0",
    "nuxt": "3.12.1",
    "pinia": "2.1.7",
    "rimraf": "5.0.7",
    "sass": "1.77.5",
    "sass-loader": "14.2.1",
    "vite-plugin-vuetify": "2.0.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "homepage": "https://github.com/EDM115-org/Tab-Magiques",
  "keywords": [
    "nuxt",
    "vuetify",
    "escalade",
    "senescalade"
  ],
  "license": "MIT",
  "name": "senescalade",
  "overrides": {
    "@nuxt/devtools": "1.3.3",
    "eslint": "9.5.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EDM115-org/Tab-Magiques.git"
  },
  "scripts": {
    "analyze": "rimraf .output && nuxi analyze",
    "build": "rimraf .output && nuxi build",
    "dev": "nuxi dev -p 8000",
    "dev-expose": "nuxi dev -p 8000 --host",
    "dev-docker": "docker compose --profile dev up -d --wait",
    "generate": "nuxi generate",
    "i": "npm i --no-audit --no-fund",
    "i-clean": "rimraf package-lock.json node_modules && npm run i",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "preview": "nuxi preview",
    "prod-docker": "docker compose --profile prod up -d --wait",
    "start": "npm run build && cross-env PORT=8000 nuxi start",
    "stop-dev-docker": "docker compose --profile dev stop",
    "stop-prod-docker": "docker compose --profile prod stop",
    "upgrade": "nuxi upgrade --force",
    "zellij-dev": "export $(cat .env | xargs) && cross-env-shell DEV_SHELL=$(basename $SHELL) \"zellij --layout .zellij.dev.kdl && npm run stop-dev-docker\"",
    "zellij-prod": "export $(cat .env | xargs) && cross-env-shell DEV_SHELL=$(basename $SHELL) \"zellij --layout .zellij.prod.kdl && npm run stop-prod-docker\""
  },
  "type": "module",
  "version": "1.0.0"
}
